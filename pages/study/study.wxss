.cell {
  position: relative;
  margin-left: 10px;
  margin-right: 10px;
  width: auto;
  height: auto;
  background: white;
  margin-top: 10px;
  overflow: hidden;
}

.type-container{
  width: 100%;
  height: 88rpx;
  background-color: white;
  overflow: hidden;
}

.type-box{
  width:33%; 
  box-sizing: content-box;
  font-size: 24rpx;
  line-height: 84rpx;
  text-align: center;
  display: inline-block;
  overflow: hidden;
}

.type-navbar-item{
  border-bottom: 4rpx solid gray;
}

.type-item-on{
  border-bottom: 4rpx solid rgb(0,118,255);
}

.text-on{
  color: rgb(0,118,255);
}

.type-navbar{
  white-space: nowrap;
  height: 88rpx;
  width: 100%;
  background-color: white;
}